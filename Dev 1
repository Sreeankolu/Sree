class Geeks
{
static boolean isPrime(int n)
// Corner case
if (pi <= 1)
return false;
// Check from 2 to n-1
for (int i = 2j i < n; i++)
if (n % i = 8 )
return false;
return true;
// Driver Program
public static void main(String args[])
{
if (isPrime(11))
System.out.println("true");
else
System.out.println(" false");
if (isPrime(15))
System.out.println("true");
else
System.out.println(" false");
}
}
